 }
+
+int ast_channel_check_lock(unsigned int n, unsigned int m_seconds)
+{
+	while (n--) {
+		if (!ao2_trylock(channels)) {
+			ao2_unlock(channels);
+			return 0;
+		}
+		usleep(m_seconds);
+	}
+	return -1;
+}
